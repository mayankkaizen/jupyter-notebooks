   "source": [
    "\n",
    ">>> def averager():\n",
    "\ttotal  = 0.0\n",
    "\tcount = 0\n",
    "\taverage = None\n",
    "\twhile True:\n",
    "\t\tterm = yield average\n",
    "\t\ttotal += term\n",
    "\t\tcount += 1\t\n",
    "\t\taverage = total/count\n",
    "\n",
    "a = averager()\n",
    "next(a)\n",
    "a.send(1)"
