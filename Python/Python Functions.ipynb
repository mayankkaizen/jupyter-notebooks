   "source": [
    "def decoratorf(func):\n",
    "    def wrapper(name):\n",
    "        print(\"%s :)\" %(func(name)))\n",
    "    return wrapper    \n",
    "        \n",
    "#printf = decoratorf(printf)\n",
    "\n",
    "@decoratorf\n",
    "def printf(name):\n",
    "    return \"Hi, %s\" %name\n",
    "\n",
    "@decoratorf\n",
    "def printx(name):\n",
    "    return \"Hi there %s\" %name\n",
    "\n",
    "printf('mayank')\n",
    "printx('raj')\n"
